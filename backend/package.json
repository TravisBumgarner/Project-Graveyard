{
  "name": "backend",
  "author": "Travis Bumgarner",
  "scripts": {
    "sd:be": "export NODE_ENV=development && ts-node-dev ./index.ts",
    "build": "export NODE_ENV=production && rm -rf ./build && tsc",
    "sp:be": "export NODE_ENV=production && node build/index.js",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js --config ./src/db/ormconfig.ts",
    "migration:generate": "npm run typeorm migration:generate -- -n",
    "migration:run": "npm run typeorm migration:run",
    "migration:revert": "npm run typeorm migration:revert",
    "migration:run:dev": "export NODE_ENV=development && npm run migration:run",
    "migration:run:production": "export NODE_ENV=production && npm run migration:run",
    "precommit": "echo \"---------------------------- NOTHING TO SEE HERE ON BACKEND ----------------------------\"",
    "deploy": "npm run build && gcloud config set project worksheets-9b1ad && gcloud builds submit --tag gcr.io/worksheets-9b1ad/backend && gcloud run deploy backend --image gcr.io/worksheets-9b1ad/backend --platform managed --region us-central1"
  },
  "keywords": [],
  "license": "ISC",
  "dependencies": {
    "apollo-server": "^3.6.3",
    "body-parser": "^1.19.2",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.2",
    "express-graphql": "^0.12.0",
    "firebase-admin": "^10.0.2",
    "pg": "^8.7.3",
    "runtypes": "^6.5.0",
    "sqlite3": "^5.0.2",
    "ts-node": "^10.4.0",
    "ts-node-dev": "^1.1.8",
    "typeorm": "^0.2.41",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^17.0.17",
    "@types/uuid": "^8.3.4"
  }
}